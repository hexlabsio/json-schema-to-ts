{
  "name": "@hexlabs/json-schema-to-ts",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --ci --runInBand --coverage --reporters=default --reporters=jest-junit",
    "lint": "eslint **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "template",
  "bugs": {
    "url": "https://github.com/hexlabsio/typescript-template/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm outdated"
    }
  },
  "jest": {
    "transform": {
      ".(ts)": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "homepage": "",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^16.18.8",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^6.0.0",
    "jest": "^29.0.3",
    "jest-junit": "^10.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8"
  },
  "dependencies": {
    "@hexlabs/typescript-generator": "^1.1.7",
    "prettier": "^2.8.1"
  }
}
